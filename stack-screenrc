sessionname stack
hardstatus alwayslastline '%{= .} %-Lw%{= .}%> %n%f %t*%{= .}%+Lw%< %-=%{g}(%{d}%H/%l%{g})'
setenv PROMPT_COMMAND /bin/true
screen -t shell bash
screen -t w-sync bash
stuff "cd /opt/stack/whale && /opt/stack/whale/bin/whale-init --config-file /etc/whale/whale.conf"
screen -t w-api bash
stuff "sleep 10;cd /opt/stack/whale && /opt/stack/whale/bin/whale-server --config-file /etc/whale/whale.conf"
