stuff "cd /opt/stack/nova && nova-api --config-dir=/etc/nova"
screen -t q-svc bash
stuff "sleep 25;cd /opt/stack/neutron && python /usr/local/bin/neutron-server --config-file /etc/neutron/neutron.conf --config-file /etc/neutron/plugins/janus/janus.ini"
screen -t q-agt bash
stuff "sleep 30;cd /opt/stack/neutron && python /opt/stack/neutron/neutron/plugins/janus/agent/janus_neutron_agent.py --config-file /etc/neutron/neutron.conf --config-file /etc/neutron/plugins/janus/janus.ini"
screen -t q-dhcp bash
stuff "sleep 30;cd /opt/stack/neutron && python /usr/local/bin/neutron-dhcp-agent --config-file /etc/neutron/neutron.conf --config-file=/etc/neutron/dhcp_agent.ini"
